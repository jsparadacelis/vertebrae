# S3 Configuration
S3_BUCKET=vertebrae-artifacts
MODEL_CACHE_DIR=/tmp/model_cache

# Model Selection (yolo or maskrcnn)
DEFAULT_MODEL=yolo

# YOLO Model Configuration
YOLO_MODEL_KEY=yolo_model.pt

# Mask R-CNN Model Configuration
MASKRCNN_MODEL_KEY=model_final.pth
MASKRCNN_BACKBONE=COCO-InstanceSegmentation/mask_rcnn_R_50_FPN_3x.yaml

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=1

# Model Inference Configuration
CONFIDENCE_THRESHOLD=0.5
NMS_THRESHOLD=0.5
MAX_DETECTIONS=100
DEVICE=cpu

# Logging
LOG_LEVEL=INFO
